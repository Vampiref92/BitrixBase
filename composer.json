{
  "name": "vf92/bitrix-base",
  "description": "base bitrix classes. Bitrix minimal version 16.5.0",
  "homepage": "https://github.com/Vampiref92/BitrixBase",
  "type": "library",
  "license": "proprietary",
  "keywords": [
    "bitrix"
  ],
  "authors": [
    {
      "name": "Frolov Sergey",
      "email": "vampiref92@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Vf92\\BitrixUtils\\": "src/BitrixUtils/",
      "Vf92\\Connections\\": "src/Connections/",
      "Vf92\\Log\\": "src/Log/",
      "Vf92\\MiscUtils\\": "src/MiscUtils/"
    }
  },
  "require": {
    "php": ">=5.6",
    "ext-ftp": "*",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.22",
    "doctrine/collections": "^1.4",
    "webarchitect609/bitrix-cache": "^1.0",
    "webarchitect609/bitrix-neverinclude": "^0.4.2"
  },
  "require-dev": {
    "dotsunited/php-cs-fixer-config" : "^1.5"
  },
  "suggest": {
    "ext-ftp": "to connect to ftp:// server"
  },
  "minimum-stability": "stable"
}